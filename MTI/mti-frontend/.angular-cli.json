{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular-admin-lte"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "target/dist",
      "assets": [
        "app/assets",
        "app/favicon.ico"
      ],
      "index": "index.html",
      "main": "app/main.ts",
      "polyfills": "app/polyfills.ts",
      "test": "app/test.ts",
      "tsconfig": "app/tsconfig.app.json",
      "testTsconfig": "app/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../node_modules/font-awesome/css/font-awesome.css",
        "../node_modules/ionicons/css/ionicons.css",
        "../node_modules/morris.js/morris.css",
        "../node_modules/prismjs/themes/prism-tomorrow.css",
        "../node_modules/admin-lte/dist/css/skins/_all-skins.css",
        "../node_modules/primeng/resources/themes/omega/theme.css",
        "../node_modules/admin-lte/dist/css/AdminLTE.css",
        "../node_modules/primeng/resources/primeng.min.css",
        "app/styles.css",
        "app/assets/bootstrap-fileinput/css/fileinput.css",
        "app/assets/bootstrap-fileinput/themes/explorer-fa/theme.css"
      ],
      "scripts": [
        "../node_modules/prismjs/prism.js",
        "../node_modules/prismjs/components/prism-typescript.js",
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/chart.js/dist/Chart.js",
        "app/assets/js/jquery.slimscroll.js",
        "../node_modules/admin-lte/dist/js/adminlte.js",
        "app/assets/bootstrap-fileinput/js/plugins/sortable.js",
        "app/assets/bootstrap-fileinput/js/fileinput.js",
        "app/assets/bootstrap-fileinput/js/locales/es.js",
        "app/assets/bootstrap-fileinput/themes/explorer-fa/theme.js",
        "app/assets/bootstrap-fileinput/themes/fa/theme.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/app/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/app/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
